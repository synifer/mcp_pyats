{
  "manifest_version": 3,
  "name": "MCP Draw.io Bridge",
  "version": "1.0",
  "description": "Bridge MCP tools with draw.io",
  "permissions": [], 
  "host_permissions": ["<all_urls>"], 
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"],
      "run_at": "document_idle", 
      "all_frames": true 
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["webscript.js"],
      "matches": ["<all_urls>"]
    }
  ]
  }